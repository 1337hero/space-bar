{
    "compilerOptions": {
        "target": "ES2021",
        "strict": true,
        "outDir": "./target",
        "lib": ["ES2021"],
        "paths": {
            "@imports/*": ["./@types/*"],
            "gi://*": ["./src/types/generated/gi/*"],
            "resource:///org/gnome/shell/*": ["./src/types/dummy/shell/*"],
            "resource:///org/gnome/Shell/Extensions/js/*": ["./src/types/dummy/shell/*"]
        }
    },
    "include": ["src/**/*.ts"],
    "exclude": ["src/types/dummy/*"]
}
